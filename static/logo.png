    <style>
        :root {
            /* Gemini-Inspired Clean Dark Theme */
            --background: 0 0% 10%;
            /* #1a1a1a */
            --foreground: 0 0% 91%;
            /* #e8eaed */

            /* Card System - Elevated surfaces */
            --card: 0 0% 18%;
            /* #2d2d2d */
            --card-foreground: 0 0% 91%;
            --card-border: 0 0% 24%;
            /* #3d3d3d */

            /* Primary - Google Blue */
            --primary: 217 91% 60%;
            /* #4285f4 */
            --primary-foreground: 0 0% 100%;
            --primary-hover: 217 91% 65%;

            /* Accent Colors */
            --ai-1: 340 82% 52%;
            /* Pink/Red */
            --ai-1-foreground: 0 0% 100%;
            --ai-2: 45 93% 47%;
            /* Amber */
            --ai-2-foreground: 0 0% 10%;
            --ai-3: 171 77% 64%;
            /* Teal */
            --ai-3-foreground: 0 0% 10%;

            /* Status Colors */
            --success: 142 71% 45%;
            --warning: 38 92% 50%;
            --error: 0 84% 60%;

            /* UI Elements */
            --secondary: 0 0% 22%;
            /* #383838 */
            --secondary-foreground: 0 0% 91%;

            --muted: 0 0% 15%;
            /* #262626 */
            --muted-foreground: 0 0% 64%;
            /* #a3a3a3 */

            --accent: 217 91% 60%;
            --accent-foreground: 0 0% 100%;

            --destructive: 0 84% 60%;
            --destructive-foreground: 0 0% 100%;

            /* Borders and inputs */
            --border: 0 0% 24%;
            /* #3d3d3d */
            --input: 0 0% 18%;
            --ring: 217 91% 60%;

            --radius: 0.75rem;
        }

        /* Dark theme remains primary with slight adjustments */
        .dark {
            --background: 222.2 84% 4.9%;
            --foreground: 210 40% 98%;
            --card: 222.2 84% 4.9%;
            --card-foreground: 210 40% 98%;
            --primary: 194 80% 48%;
            --primary-foreground: 220 15% 8%;
            --secondary: 217.2 32.6% 17.5%;
            --secondary-foreground: 210 40% 98%;
            --muted: 217.2 32.6% 17.5%;
            --muted-foreground: 215 20.2% 65.1%;
            --accent: 217.2 32.6% 17.5%;
            --accent-foreground: 210 40% 98%;
            --destructive: 0 62.8% 30.6%;
            --destructive-foreground: 210 40% 98%;
            --border: 217.2 32.6% 17.5%;
            --input: 217.2 32.6% 17.5%;
            --ring: 194 80% 48%;
        }

        body {
            font-family: 'Inter', sans-serif;
            background-color: hsl(var(--background));
            color: hsl(var(--foreground));
            line-height: 1.6;
            font-weight: 400;
            min-height: 100vh;
        }

        /* Clean card style - no blur */
        .card {
            background-color: hsl(var(--card));
            color: hsl(var(--card-foreground));
            border: 1px solid hsl(var(--border));
            border-radius: var(--radius);
            transition: all 0.2s ease;
        }

        /* Interactive card hover */
        .card:hover {
            background-color: hsl(0 0% 20%);
            border-color: hsl(0 0% 28%);
        }

        /* Clean interactive buttons */
        .btn-primary {
            background-color: hsl(var(--primary));
            color: hsl(var(--primary-foreground));
            padding: 0.625rem 1.25rem;
            border-radius: var(--radius);
            border: none;
            cursor: pointer;
            font-weight: 500;
            font-size: 0.875rem;
            transition: all 0.2s ease;
        }

        .btn-primary:hover {
            background-color: hsl(var(--primary-hover));
            transform: translateY(-1px);
        }

        .btn-primary:active {
            transform: scale(0.98);
        }

        /* Secondary button */
        .btn-secondary {
            background-color: hsl(var(--secondary));
            color: hsl(var(--foreground));
            padding: 0.625rem 1.25rem;
            border-radius: var(--radius);
            border: 1px solid hsl(var(--border));
            cursor: pointer;
            font-weight: 500;
            font-size: 0.875rem;
            transition: all 0.2s ease;
        }

        .btn-secondary:hover {
            background-color: hsl(0 0% 25%);
            border-color: hsl(0 0% 32%);
        }

        .btn-secondary:active {
            transform: scale(0.98);
        }

        /* Dashboard / API Docs button styling (reinserted) */
        .btn-dashboard {
            background: linear-gradient(90deg, #6366f1 0%, #06b6d4 100%);
            color: #fff !important;
            padding: 0.5rem 1.2rem;
            border-radius: var(--radius);
            font-weight: 600;
            border: none;
            box-shadow: 0 2px 8px -2px rgba(99, 102, 241, 0.15);
            transition: all 0.2s;
            display: flex;
            align-items: center;
        }

        .btn-dashboard:hover {
            background: linear-gradient(90deg, #4f46e5 0%, #0ea5e9 100%);
            color: #fff !important;
            transform: translateY(-2px) scale(1.03);
            box-shadow: 0 4px 16px -4px rgba(99, 102, 241, 0.18);
        }

        .btn-primary:active::after {
            animation: ripple 1s ease-out;
        }

        @keyframes ripple {
            0% {
                transform: scale(0, 0);
                opacity: 0.5;
            }

            100% {
                transform: scale(20, 20);
                opacity: 0;
            }
        }

        /* Chrome, Safari, Edge */
        ::-webkit-scrollbar {
            width: 0px;
            /* removes scrollbar space */
            background: transparent;
            /* optional: transparent track */
        }

        /* Firefox */
        html {
            scrollbar-width: none;
            /* removes scrollbar */
        }

        .btn-secondary {
            background-color: transparent;
            color: hsl(var(--secondary-foreground));
            padding: 0.6rem 1.2rem;
            border-radius: var(--radius);
            border: var(--precision-border) solid hsl(var(--border));
            cursor: pointer;
            transition: all 0.2s ease;
            font-weight: 500;
            letter-spacing: 0.5px;
        }

        .btn-secondary:hover {
            background-color: hsl(var(--secondary));
            border-color: hsl(var(--primary));
        }

        /* Scientific precision inputs */
        .input {
            background-color: hsl(var(--input));
            color: hsl(var(--foreground));
            border: var(--precision-border) solid hsl(var(--border));
            border-radius: var(--radius);
            padding: 0.6rem 0.8rem;
            transition: all 0.2s ease;
            font-family: 'Inter', monospace;
            font-size: 0.95rem;
        }

        .input:focus {
            outline: none;
            border-color: hsl(var(--primary));
            box-shadow: 0 0 0 2px hsl(var(--ring) / 0.2);
        }

        /* Technical badges with data indicator style */
        .badge {
            padding: 0.3rem 0.7rem;
            border-radius: calc(var(--radius) * 0.8);
            font-size: 0.75rem;
            font-weight: 600;
            letter-spacing: 0.5px;
            text-transform: uppercase;
        }

        .badge-primary {
            background-color: hsl(var(--primary));
            color: hsl(var(--primary-foreground));
        }

        .badge-secondary {
            background-color: hsl(var(--secondary));
            color: hsl(var(--secondary-foreground));
            border: var(--precision-border) solid hsl(var(--border));
        }

        /* AI agent specific badges */
        .badge-ai-1 {
            background-color: hsl(var(--ai-1));
            color: hsl(var(--ai-1-foreground));
        }

        .badge-ai-2 {
            background-color: hsl(var(--ai-2));
            color: hsl(var(--ai-2-foreground));
        }

        .badge-ai-3 {
            background-color: hsl(var(--ai-3));
            color: hsl(var(--ai-3-foreground));
        }

        /* Elevated glass card with cinematic shadow */
        .card-elevated {
            box-shadow:
                0 4px 24px rgba(0, 0, 0, 0.3),
                0 8px 48px rgba(139, 92, 246, 0.08);
            transition: all 0.3s cubic-bezier(0.16, 1, 0.3, 1);
        }

        .card-elevated:hover {
            transform: translateY(-4px);
            box-shadow:
                0 8px 32px rgba(0, 0, 0, 0.4),
                0 16px 64px rgba(139, 92, 246, 0.15);
            border-color: rgba(139, 92, 246, 0.3);
        }

        /* Manhattan Project data visualization elements */
        .data-line {
            height: var(--data-line);
            background: linear-gradient(90deg,
                    transparent,
                    hsl(var(--primary)),
                    transparent);
            margin: 1rem 0;
            opacity: 0.7;
        }

        /* Scientific measurement indicators */
        .measurement {
            position: relative;
            padding-left: 1.2rem;
        }

        .measurement::before {
            content: '';
            position: absolute;
            left: 0;
            top: 50%;
            transform: translateY(-50%);
            width: 0.8rem;
            height: 0.8rem;
            border: var(--precision-border) solid hsl(var(--primary));
            border-radius: 50%;
        }

        /* AI network connection visualization */
        .ai-connection {
            position: relative;
        }

        .ai-connection::after {
            content: '';
            position: absolute;
            bottom: -0.5rem;
            left: 50%;
            transform: translateX(-50%);
            width: 60%;
            height: 2px;
            background: linear-gradient(90deg,
                    transparent,
                    hsl(var(--ai-1)),
                    hsl(var(--ai-2)),
                    hsl(var(--ai-3)),
                    transparent);
            opacity: 0.6;
        }
    </style>

